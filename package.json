{
  "name": "bbproject",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "types": "tsc --noEmit",
    "lint": "next lint",
    "prepare": "husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "js-cookie": "^3.0.5",
    "next": "13.4.6",
    "next-intl": "^3.26.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^9.2.0",
    "sass": "^1.83.1",
    "styled-components": "^6.1.11",
    "styled-normalize": "^8.1.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "src/**.{js,jsx,ts,tsx}": [
      "tsc --noEmit",
